    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "@types/react": "^19.0.0",
    "eslint": "^9.13.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prisma": "^5.12.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3"
