    "lint": "eslint --cache src",
    "start": "next start",
    "studio": "prisma studio",
    "migrate-dev": "prisma migrate dev",
    "migrate": "prisma migrate deploy",
    "test": "playwright test",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^5.22.0",
    "@tanstack/react-query": "^5.66.0",
    "@trpc/client": "link:pnpm:@trpc/client@next",
    "@trpc/next": "link:pnpm:@trpc/next@next",
    "@trpc/react-query": "link:pnpm:@trpc/react-query@next",
    "@trpc/server": "link:pnpm:@trpc/server@next",
    "bcrypt": "^5.1.1",
    "bootstrap": "^5.3.3",
    "clsx": "^2.1.1",
    "fs": "0.0.1-security",
    "next": "^15.1.3",
    "next-auth": "^4.24.11",
    "postgres": "^3.4.5",
    "react": "^19.0.0",
    "react-bootstrap": "^2.10.9",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "superjson": "^1.12.4",
    "todomvc-app-css": "^2.3.0",
